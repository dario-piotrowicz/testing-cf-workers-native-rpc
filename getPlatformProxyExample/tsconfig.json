{
    "compilerOptions": {
      "lib": ["ESNext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "target": "ES2020",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "incremental": true,
      "plugins": [
        {
          "name": "next"
        }
      ],
      "paths": {
        "@/*": ["./*"]
      },
      "types": [
          "@cloudflare/workers-types/2023-07-01"
      ]
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }
  